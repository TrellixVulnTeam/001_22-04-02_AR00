rubiksUI:

<rubiksUI>:
	Widget:
	grpRubiks:
		background_color: [0,0,0,0]
		id: grpRubiks
		anchor_x: 'left'
		anchor_y: 'bottom'
		size_hint: None,None
		on_press: self._init1()
	GridLayout:
		rows: 5
		cols: 3
		orientation: 'lr-bt'
		Button:
			id: dragg
			size_hint: None,None
			on_press: root.move(self)
			on_touch_up: root.release(self)
			Button:
				text: 'Kyung Kyung'
				width: self.parent.width/2
				height: self.parent.height/2
				x: self.parent.x + self.parent.width
				y: self.parent.y + self.parent.height/4
		Button:
			size_hint: None,None
			on_press: print(dragg.pos)
			on_touch_up: root.release(self)
		Button:
			size_hint: None,None
			on_press: print(dragg.pos)
			on_touch_up: root.release(self)
		Button:
			size_hint: None,None
			on_press: print(dragg.pos)
